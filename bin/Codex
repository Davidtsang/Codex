"""
Runner script for Codex
"""

#!/usr/bin/env python
import sys, os
from PyQt4.QtCore import *
from PyQt4.QtGui import *

def main():
    app = QApplication(sys.argv)
    sys.path.append(os.path.dirname(os.path.dirname(os.path.abspath(__file__))))
    import src.config
    src.config.m.show()
    app.exec_()

if __name__ == '__main__':
    main()
